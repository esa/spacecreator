#define SimpleChoice_alt1_PRESENT 1
#define SimpleChoice_alt2_PRESENT 2
inline Type1_generate_value(value)
{
    if
    ::  atomic {
        select (value : 0 .. 3);
    }
    fi;
}
inline Type2_generate_value(value)
{
    if
    ::  atomic {
        select (value : 9 .. 13);
    }
    fi;
}
inline SimpleChoice_generate_value(value)
{
    if
    ::  true;
        Type1_generate_value(value.alt1);
        value.selection = SimpleChoice_alt1_PRESENT;
    ::  true;
        Type2_generate_value(value.alt2);
        value.selection = SimpleChoice_alt2_PRESENT;
    fi;
}
