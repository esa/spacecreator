        input startProcess(x);
            /* CIF decision (388, 241), (115, 50) */
            decision x;
                /* CIF ANSWER (313, 311), (70, 24) */
                (0):
                    /* CIF NEXTSTATE (313, 355), (70, 35) */
                    NEXTSTATE -;
                /* CIF ANSWER (585, 323), (77, 24) */
