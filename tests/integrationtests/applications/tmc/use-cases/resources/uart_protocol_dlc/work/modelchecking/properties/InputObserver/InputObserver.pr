    endchannel;
    block InputObserver;
        signalroute r
            from env to InputObserver with sig;
        connect c and r;
        /* CIF PROCESS (259, 87), (150, 75) */
        process InputObserver;
            /* CIF TEXT (0, 57), (222, 140) */
            monitor st System_State;
            monitor event Observable_Event;
