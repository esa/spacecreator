system InputObserver;
    /* CIF TEXT (144, 214), (399, 140) */
    use datamodel comment 'observer.asn';
    
    signal sig(UINT8) renames input PHYTxChar to ErroredChannel;
    /* CIF ENDTEXT */
    channel c
