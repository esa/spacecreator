/* CIF PROCESS (250, 150), (150, 75) */
process Thething;
    /* CIF TEXT (631, 34), (267, 140) */
    -- Text area for declarations and comments
    
    dcl arg MyString;
    dcl result MyString;
    /* CIF ENDTEXT */
    /* CIF START (320, 10), (70, 35) */
    START;
        /* CIF NEXTSTATE (320, 60), (70, 35) */
        NEXTSTATE Wait;
    /* CIF state (451, 10), (70, 35) */
    state Wait;
        /* CIF input (446, 65), (74, 35) */
        input Tick(arg);
            /* CIF task (437, 120), (93, 35) */
            task result := arg;
            /* CIF NEXTSTATE (449, 175), (70, 35) */
            NEXTSTATE Wait;
    endstate;
endprocess Thething;