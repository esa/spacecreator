/* CIF PROCESS (250, 150), (150, 75) */
process Model;
    /* CIF TEXT (536, 12), (132, 44) */
    dcl x MyInteger;
    dcl y MyInteger;
    /* CIF ENDTEXT */
    /* CIF START (320, 10), (70, 35) */
    START;
        /* CIF NEXTSTATE (320, 60), (70, 35) */
        NEXTSTATE S1;
    /* CIF state (450, 10), (70, 35) */
    state S1;
        /* CIF input (443, 65), (78, 35) */
        input trigger(x);
            /* CIF task (446, 120), (72, 35) */
            task y := x;
            /* CIF output (445, 170), (74, 35) */
            output action(y);
            /* CIF NEXTSTATE (448, 225), (70, 35) */
            NEXTSTATE S1;
    endstate;
endprocess Model;