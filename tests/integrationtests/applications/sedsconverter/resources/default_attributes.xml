<Attrs>
    <Attr name="name">
        <Type>
            <String validator="[a-zA-Z0-9_]+"/>
        </Type>
        <Scopes/>
    </Attr>
    <Attr name="value">
        <Type>
            <String validator="[a-zA-Z0-9_]+"/>
        </Type>
        <Scopes/>
    </Attr>
    <Attr name="language">
        <Scopes>
            <Function/>
        </Scopes>
        <Type>
            <Enumeration>
                <Entry value="C"/>
                <Entry value="ADA"/>
                <Entry value="SDL"/>
            </Enumeration>
        </Type>
    </Attr>
    <Attr name="is_type">
        <Type>
            <Enumeration>
                <Entry value="YES"/>
                <Entry value="NO"/>
            </Enumeration>
        </Type>
        <Scopes>
            <Function/>
        </Scopes>
    </Attr>
    <Attr name="instance_of">
        <Type>
            <String validator="[a-zA-Z0-9_]+"/>
        </Type>
        <Scopes>
            <Function>
                <AttrValidator name="is_type" value="False"/>
            </Function>
        </Scopes>
    </Attr>
    <Attr name="kind">
        <Scopes>
            <Required_Interface/>
            <Provided_Interface/>
        </Scopes>
        <Type>
            <Enumeration>
                <Entry value="CYCLIC_OPERATION"/>
                <Entry value="SPORADIC_OPERATION"/>
                <Entry value="PROTECTED_OPERATION"/>
                <Entry value="UNPROTECTED_OPERATION"/>
            </Enumeration>
        </Type>
    </Attr>
    <Attr name="period">
        <Type>
            <String validator="\\d+"/>
        </Type>
        <Scopes>
            <Required_Interface>
                <AttrValidator name="kind" value="CYCLIC_OPERATION"/>
            </Required_Interface>
            <Provided_Interface>
                <AttrValidator name="kind" value="CYCLIC_OPERATION"/>
            </Provided_Interface>
        </Scopes>
    </Attr>
    <Attr name="wcet">
        <Type>
            <String validator="\\d+"/>
        </Type>
        <Scopes>
            <Required_Interface>
                <AttrValidator name="kind" value="CYCLIC_OPERATION"/>
            </Required_Interface>
            <Provided_Interface>
                <AttrValidator name="kind" value="CYCLIC_OPERATION"/>
            </Provided_Interface>
        </Scopes>
    </Attr>
    <Attr name="queue_size">
        <Type>
            <String validator="\\d+"/>
        </Type>
        <Scopes>
            <Required_Interface>
                <AttrValidator name="kind" value="CYCLIC_OPERATION"/>
            </Required_Interface>
            <Provided_Interface>
                <AttrValidator name="kind" value="CYCLIC_OPERATION"/>
            </Provided_Interface>
        </Scopes>
    </Attr>
    <Attr name="Taste::coordinates" type="property" visible="false">
        <Type>
            <String validator="[0-9 ]+"/>
        </Type>
        <Scopes/>
    </Attr>
    <Attr name="Taste::InnerCoordinates" type="property" visible="false">
        <Type>
            <String validator="[0-9 ]+"/>
        </Type>
        <Scopes/>
    </Attr>
    <Attr name="Taste::RootCoordinates" type="property" visible="false">
        <Type>
            <String validator="[0-9 ]+"/>
        </Type>
        <Scopes>
            <Function/>
        </Scopes>
    </Attr>
    <Attr name="Taste::InheritPI" type="property">
        <Type>
            <Boolean/>
        </Type>
        <Scopes>
            <Required_Interface/>
        </Scopes>
    </Attr>
    <Attr name="Taste::Autonamed" type="property">
        <Type>
            <Boolean/>
        </Type>
        <Scopes>
            <Required_Interface/>
        </Scopes>
    </Attr>
    <Attr name="IntForProv">
        <Type>
            <Integer/>
        </Type>
        <Scopes>
            <Provided_Interface/>
        </Scopes>
    </Attr>
</Attrs>