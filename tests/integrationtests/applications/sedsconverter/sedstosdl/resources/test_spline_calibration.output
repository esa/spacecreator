process Component;
    dcl Megavariable IntegerType;
    dcl RawSplinePoints1 SplinePointsArray;
    dcl CalibratedSplinePoints1 SplinePointsArray;

    procedure InitRawSplinePoints1;
        START;
        return ;
    endprocedure;

    procedure InitCalibratedSplinePoints1;
        START;
        return ;
    endprocedure;

    procedure MegaActivity;
        fpar
            in Activityarg IntegerType;
        START;
            task Megavariable := Megavariable;
        return ;
    endprocedure;
    procedure InterfacePi_MegaCommand_Pi;
        fpar
            in Commandarg IntegerType;
        START;
            task Megavariable := Commandarg;
            call MegaActivity(Megavariable);
        return ;
    endprocedure;
    procedure Get_InterfacePi_Param_Pi;
        fpar
            in/out Param IntegerType;
        START;
            task Param := Megavariable;
        return ;
    endprocedure;
    procedure Set_InterfacePi_Param_Pi;
        fpar
            in Param IntegerType;
        START;
            task Megavariable := Param;
        return ;
    endprocedure;
    START;
        task call InitRawSplinePoints1;
        task call InitCalibratedSplinePoints1;
        NEXTSTATE State1;
    state State1;
    endstate;
    state State2;
        input InterfacePi_MegaCommand_Pi;
            NEXTSTATE State2;
    endstate;
    state State3;
    endstate;
endprocess Component;
