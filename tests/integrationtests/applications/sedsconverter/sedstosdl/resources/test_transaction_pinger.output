/* CIF Process (0, 0), (800, 100) */
process Pinger;
    /* CIF Text (0, 150), (400, 500) */
    dcl Pingvar Number;
    /* CIF ENDTEXT */
    /* CIF Procedure (0, 900), (800, 50) */
    procedure DoPingSmall;
        /* CIF Text (0, 0), (400, 500) */
        fpar
            in Pingvar Number;
        /* CIF ENDTEXT */
        /* CIF Start (0, 750), (200, 75) */
        START;
            /* CIF ProcedureCall (0, 937), (100, 50) */
            call Ping_PingCommand_Ri(Pingvar, "MakeItSmall");
    return;
    endprocedure;
    /* CIF Procedure (0, 975), (800, 50) */
    procedure Clk_Tick_Pi;
        /* CIF Start (0, 0), (200, 75) */
        START;
            /* CIF ProcedureCall (0, 187), (100, 50) */
            call DoPingSmall(1);
    return;
    endprocedure;
    /* CIF Start (0, 1050), (200, 75) */
    START;
        /* CIF NextState (0, 1237), (200, 50) */
        NEXTSTATE s1;
    /* CIF State (0, 1312), (200, 50) */
    state s1;
        /* CIF Input (0, 1312), (200, 50) */
        input Clk_Tick_Pi;
            /* CIF NextState (0, 1387), (200, 50) */
            NEXTSTATE s1;
    endstate;
endprocess Pinger;
