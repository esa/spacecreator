<?xml version="1.0" encoding="utf-8"?>
<DataSheet xmlns="http://www.ccsds.org/schema/sois/seds" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.ccsds.org/schema/sois/seds seds.xsd">
  <Package name="com/n7space/Package1">
    <DataTypeSet>
      <ArrayDataType name="BasicArray1D" dataTypeRef="com/n7space/Package2/ArrayItem">
        <LongDescription>A 1-D array with maximum size</LongDescription>
        <DimensionList>
          <Dimension size="15" />
        </DimensionList>
      </ArrayDataType>
      <ArrayDataType name="ArrayWithIndexDataRef" dataTypeRef="com/n7space/Package2/ArrayItem">
        <DimensionList>
          <Dimension indexTypeRef="com/n7space/Package2/IndexItem" />
        </DimensionList>
      </ArrayDataType>
      <ContainerDataType name="BasicContainer">
        <LongDescription>A basic container data type</LongDescription>
        <EntryList>
          <Entry name="fieldA" type="com/n7space/Package2/FieldItem" />
          <Entry name="fieldB" type="com/n7space/Package2/FieldItem" />
          <Entry name="fieldC" type="com/n7space/Package2/FieldItem" />
          <Entry name="lengthEntry" type="com/n7space/Package2/LengthItem" />
          <ListEntry name="listEntry" type="com/n7space/Package2/ListItem" listLengthField="lengthEntry"/>
        </EntryList>
      </ContainerDataType>
      <ContainerDataType name="ChildContainer" abstract="false" baseType="BasicContainer">
        <ConstraintSet>
          <TypeConstraint entry="fieldA" type="com/n7space/Package2/ConstraintItem" />
        </ConstraintSet>
      </ContainerDataType>
      <SubRangeDataType name="SubRangeData" baseType="com/n7space/Package2/SubRangeItem">
        <Range>
          <MinMaxRange rangeType="atLeast" min="10" />
        </Range>
      </SubRangeDataType>
    </DataTypeSet>
  </Package>
  <Package name="com/n7space/Package2">
    <DataTypeSet>
      <IntegerDataType name="ArrayItem">
        <IntegerDataEncoding encoding="unsigned" sizeInBits="8" />
        <Range>
          <MinMaxRange rangeType="inclusiveMinInclusiveMax" min="0" max="255" />
        </Range>
      </IntegerDataType>
      <IntegerDataType name="IndexItem">
        <IntegerDataEncoding encoding="unsigned" sizeInBits="8" />
        <Range>
          <MinMaxRange rangeType="inclusiveMinInclusiveMax" min="0" max="16" />
        </Range>
      </IntegerDataType>
      <IntegerDataType name="FieldItem">
        <IntegerDataEncoding encoding="unsigned" sizeInBits="8" />
        <Range>
          <MinMaxRange rangeType="inclusiveMinInclusiveMax" min="0" max="255" />
        </Range>
      </IntegerDataType>
      <IntegerDataType name="LengthItem">
        <IntegerDataEncoding encoding="unsigned" sizeInBits="8" />
        <Range>
          <MinMaxRange rangeType="inclusiveMinInclusiveMax" min="0" max="10" />
        </Range>
      </IntegerDataType>
      <IntegerDataType name="ListItem">
        <IntegerDataEncoding encoding="unsigned" sizeInBits="8" />
        <Range>
          <MinMaxRange rangeType="inclusiveMinInclusiveMax" min="0" max="255" />
        </Range>
      </IntegerDataType>
      <IntegerDataType name="ConstraintItem">
        <IntegerDataEncoding encoding="unsigned" sizeInBits="8" />
        <Range>
          <MinMaxRange rangeType="inclusiveMinInclusiveMax" min="0" max="10" />
        </Range>
      </IntegerDataType>
      <IntegerDataType name="ChildItem">
        <IntegerDataEncoding encoding="unsigned" sizeInBits="8" />
        <Range>
          <MinMaxRange rangeType="inclusiveMinInclusiveMax" min="0" max="255" />
        </Range>
      </IntegerDataType>
      <IntegerDataType name="SubRangeItem">
        <IntegerDataEncoding encoding="unsigned" sizeInBits="8" />
        <Range>
          <MinMaxRange rangeType="inclusiveMinInclusiveMax" min="0" max="255" />
        </Range>
      </IntegerDataType>
    </DataTypeSet>
  </Package>
</DataSheet>
