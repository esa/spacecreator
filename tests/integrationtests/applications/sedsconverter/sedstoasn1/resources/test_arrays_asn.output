COM-N7SPACE-ARRAYS DEFINITIONS ::= BEGIN
-- A sample data item to be embedded within other structures
DataItem ::= INTEGER(0 .. 255)
IndexItem ::= INTEGER(0 .. 16)
-- A 1-D array with maximum size
BasicArray1D ::= SEQUENCE (SIZE(15)) OF DataItem(0 .. 255)
-- A 2D array
Matrix4x4 ::= SEQUENCE (SIZE(4)) OF SEQUENCE (SIZE(4)) OF DataItem(0 .. 255)
ArrayWithIndexDataRef ::= SEQUENCE (SIZE(16)) OF DataItem(0 .. 255)
END

