{
    realization CHOICE
    {
        realizationBasicContainer SEQUENCE
        {
            fieldA FieldItem(0 .. 255),
            fieldB FieldItem(0 .. 255),
            fieldC FieldItem(0 .. 255),
            listEntry SEQUENCE (SIZE(0 .. 10)) OF ListItem(0 .. 255)
