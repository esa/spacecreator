/* CIF PROCESS (250, 150), (150, 75) */
process Sensor;
    /* CIF TEXT (323, 23), (170, 140) */
    DCL x Number;
    /* CIF ENDTEXT */
    /* CIF START (518, 38), (70, 35) */
    START;
        /* CIF task (518, 93), (70, 35) */
        task x := 1.0;
        /* CIF NEXTSTATE (518, 143), (70, 35) */
        NEXTSTATE Wait;
    /* CIF state (654, 41), (70, 35) */
    state Wait;
        /* CIF input (654, 96), (70, 35) */
        input trigger;
            /* CIF task (647, 151), (82, 35) */
            task x := x * 2.0;
            /* CIF output (647, 206), (83, 35) */
            output reading(x);
            /* CIF NEXTSTATE (654, 261), (70, 35) */
            NEXTSTATE Wait;
    endstate;
endprocess Sensor;
