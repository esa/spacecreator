        <Scopes>
            <Required_Interface/>
            <Provided_Interface/>
        </Scopes>
    </Attr>
    <Attr label="Layer" name="layer" visible="true" editable="false">
        <Type>
            <String validator="[a-zA-Z][a-zA-Z0-9_]*" />
        </Type>
        <Scopes>
            <Provided_Interface/>
            <Required_Interface/>
        </Scopes>
    </Attr>
    <Attr name="is_type">
        <Type>
            <Enumeration defaultValue="NO">
                <Entry value="YES"/>
                <Entry value="NO"/>
            </Enumeration>
        </Type>
        <Scopes>
            <Function/>
        </Scopes>
    </Attr>
    <Attr name="instance_of">
        <Type>
            <String validator="[a-zA-Z][a-zA-Z0-9_]*"/>
        </Type>
        <Scopes>
            <Function>
                <AttrValidator name="is_type" value="NO"/>
            </Function>
        </Scopes>
    </Attr>
