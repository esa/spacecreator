<Attrs>
    <Attr label="Name" name="name">
        <Type>
            <String validator="[a-zA-Z][a-zA-Z0-9_]*"/>
        </Type>
    </Attr>
    <Attr label="Kind" name="kind">
        <Type>
            <Enumeration defaultValue="Sporadic">
                <Entry value="Any"/>
                <Entry value="Cyclic"/>
                <Entry value="Sporadic"/>
                <Entry value="Protected"/>
                <Entry value="Unprotected"/>
