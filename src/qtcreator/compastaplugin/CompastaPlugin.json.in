    $$dependencyList,

    \"Mimetypes\" : [
        \"<?xml version=\'1.0\' encoding=\'UTF-8\'?>\",
        \"<mime-info xmlns=\'http://www.freedesktop.org/standards/shared-mime-info\'>\",
        \"    <mime-type type=\'text/x-slim\'>\",
        \"        <sub-class-of type=\'text/plain\'/>\",
        \"        <comment>AADL/Slim Specification Format</comment>\",
        \"        <glob pattern=\'*.slim\'/>\",
        \"        <glob pattern=\'*.aadl\'/>\",
        \"    </mime-type>\",
        \"</mime-info>\"
    ]
