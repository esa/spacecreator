{
    \"Name\" : \"SpaceCreatorPlugin\",
    \"Version\" : \"$$PLUGIN_VERSION\",
    \"CompatVersion\" : \"$$QTCREATOR_COMPAT_VERSION\",
    \"Vendor\" : \"$$ORGANISATION\",
    \"Copyright\" : \"(C) 2021 European Space Agency\",
    \"License\" : \"LGPL 2.1\",
    \"Category\" : \"Modeling\",
    \"Description\" : \"Visual Editor for MSC (message sequence diagram) and AADL files\",
    \"Url\" : \"$$ORGANISATION_DOMAIN",
    $$dependencyList,

    \"Mimetypes\" : [
        \"<?xml version=\'1.0\' encoding=\'UTF-8\'?>\",
        \"<mime-info xmlns=\'http://www.freedesktop.org/standards/shared-mime-info\'>\",
        \"    <mime-type type=\'text/vnd.msc\'>\",
        \"        <sub-class-of type=\'text/msc\'/>\",
        \"        <comment>MSC Model File</comment>\",
        \"        <glob pattern=\'*.msc\'/>\",
        \"    </mime-type>\",
        \"    <mime-type type=\'application/xml\'>\",
        \"        <sub-class-of type=\'text/plain\'/>\",
        \"        <comment>IV Model File</comment>\",
        \"        <glob pattern=\'*interfaceview.xml\'/>\",
        \"    </mime-type>\",
        \"</mime-info>\"
    ]
}
